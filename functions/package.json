    "crypto-js": "^4.2.0",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.9.0",
    "markdown-it": "^14.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^20.9.0",
    "typescript": "^5.2.2"
  },
  "private": true
